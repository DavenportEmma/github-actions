<?xml version="1.0" encoding="UTF-8"?>
<Package>
    <DisplayName>CodeFusion Studio VSCode Extension</DisplayName>
    <Description>
        The CodeFusion Studio extension for Visual Studio Code provides
        build and debug support for Analog Devices microcontrollers.
    </Description>
    <Version>1.1.7</Version>
    <ReleaseDate>2025-05-07</ReleaseDate>
    <Default>true</Default>
    <Dependencies>com.analog.codefusion.base,com.analog.codefusion.cfsutil,com.analog.codefusion.plugins,com.analog.codefusion.catalog,com.analog.codefusion.python,com.analog.codefusion.make,com.analog.codefusion.openocd</Dependencies>
    <Operations>
        <Operation name="Extract">
            <Argument>@TargetDir@/VSCode</Argument>
            <Argument>cfs-ide.zip</Argument>
        </Operation>
    </Operations>
</Package>
