<?xml version="1.0" encoding="UTF-8"?>
<Package>
    <DisplayName>Bare-Metal ARM Toolchain</DisplayName>
    <Description>
        GCC-based toolchain for assembly, C and C++ programming
        targeting bare-metal 32-bit ARM cores.
    </Description>
    <Version>14.2-1</Version>
    <ReleaseDate>2023-07-28</ReleaseDate>
    <Default>true</Default>
    <Dependencies>com.analog.codefusion.base</Dependencies>
    <Operations>
        <Operation name="Extract">
            <Argument>@TargetDir@/Tools/gcc</Argument>
            <Argument>arm-gnu-toolchain-arm-none-eabi.tar.xz</Argument>
        </Operation>
    </Operations>
</Package>
